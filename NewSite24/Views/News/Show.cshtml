@using NewSite24.EF

@{
    Layout = "Master.cshtml";
}

    @foreach (Station p in ViewBag.Stations)
    {
<div class="single-blog-post d-flex align-items-center mb-50">
    <div class="post-thumb">
        <img src="/images/@(p.Id)_.jpg" alt="@p.Name" />
    </div>
    <div class="post-data">
        <a href="#" class="post-title">
            <a href="/News/Station/@p.Path">
                <h2>@p.Name</h2>
            </a>
        </a>
    </div>
</div>
    }
